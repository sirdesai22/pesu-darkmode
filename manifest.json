{
  "manifest_version": 3,
  "name": "PESU DARK MODE",
  "version": "1.0",
  "description": "Toggle custom CSS on any PESU website.",
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "PESU DARK MODE",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "and.png"
    }
  }
}