{
  "manifest_version": 3,
  "name": "Style Toggle Extension",
  "version": "1.0",
  "description": "Toggle custom CSS on any website.",
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Toggle Styles",
    "default_popup": "popup.html"
  }
}